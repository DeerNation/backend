{
  "name": "socketcluster-sample",
  "description": "A sample SocketCluster app",
  "version": "1.0.0",
  "contributors": [
    {
      "name": "Jonathan Gros-Dubois",
      "email": "grosjona@yahoo.com.au"
    }
  ],
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "ajv": "^6.5.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect": "^3.6.6",
    "debug": "^3.1.0",
    "dgraph-js": "^1.2.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "glob": "^7.1.2",
    "google-protobuf": "^3.6.0",
    "got": "^8.3.2",
    "grpc": "^1.13.0",
    "i18n": "^0.8.3",
    "ical": "^0.5.0",
    "metascraper": "^3.11.8",
    "minimist": "^1.2.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "node-babel": "^0.1.2",
    "node-cron": "^1.2.1",
    "node-gcm": "^1.0.2",
    "node-ical": "^0.7.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "sc-crud-rethink": "^6.0.2",
    "sc-errors": "^1.4.0",
    "sc-framework-health-check": "^2.0.0",
    "sc-hot-reboot": "^1.0.0",
    "scc-broker-client": "^6.0.1",
    "semver": "^5.5.0",
    "serve-static": "^1.13.2",
    "socketcluster": "^14.0.4",
    "socketcluster-client": "^13.0.1",
    "uuid": "^3.3.2",
    "winston": "^3.0.0",
    "xxhash": "^0.2.4"
  },
  "keywords": [
    "websocket",
    "server",
    "realtime",
    "cluster",
    "scalable"
  ],
  "readmeFilename": "README.md",
  "scripts": {
    "start": "ENV=production node server.js",
    "predev": "cd frontend/app && qx compile --watch &",
    "dev": "ENV=dev node server.js",
    "backend": "ENV=dev node --trace-warnings server.js",
    "postdev": "killall -9 qx",
    "start:docker": "node dockerwait.js && node ${SOCKETCLUSTER_MASTER_CONTROLLER:-server.js}",
    "lint": "standard --verbose | snazzy",
    "test": "ENV=dev nyc --reporter=html --reporter=text-summary mocha --recursive",
    "debug-test": "ENV=dev mocha --recursive",
    "deploy-mobile": "npm run compile-frontend-app && cd app/ && cordova run android --device && cd ../"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "grunt": "^1.0.3",
    "grunt-banner": "^0.6.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.1",
    "sinon": "^6.1.3",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "ignore": [
      "app/**",
      "gen/**",
      "node_modules/**"
    ],
    "globals": [
      "app",
      "qx",
      "dialog",
      "socketCluster",
      "wampSocketCluster",
      "showdown",
      "firebase",
      "PushNotification",
      "scCodecMinBin",
      "$",
      "proto",
      "jspb"
    ]
  }
}
